# Production Environment Configuration
# Backend is hosted on Railway (separate domain)

# Backend API URL - MUST be set in Vercel environment variables
# VITE_API_BASE_URL=https://calling-agent-with-bolna-production.up.railway.app

# WebSocket URL
# VITE_WS_URL=wss://calling-agent-with-bolna-production.up.railway.app

# API timeout (milliseconds)
VITE_API_TIMEOUT=30000

# Environment
VITE_NODE_ENV=production
VITE_ENV=production

# Debugging (set to false in production)
VITE_DEBUG=false
VITE_LOG_LEVEL=error

# CORS
VITE_ENABLE_CORS=true

# ElevenLabs API Key (if needed on frontend)
# VITE_XI_API_KEY=your_production_key_here
