# Production Environment Configuration
# Set these in your Vercel deployment settings

# Backend API URL - REQUIRED
# This should point to your backend server
VITE_API_BASE_URL=https://agenttest.sniperthink.com

# WebSocket URL
VITE_WS_URL=wss://agenttest.sniperthink.com

# API timeout (milliseconds)
VITE_API_TIMEOUT=30000

# Environment
VITE_NODE_ENV=production
VITE_ENV=production

# Debugging (set to false in production)
VITE_DEBUG=false
VITE_LOG_LEVEL=error

# CORS
VITE_ENABLE_CORS=true

# ElevenLabs API Key (if needed on frontend)
# VITE_XI_API_KEY=your_production_key_here
